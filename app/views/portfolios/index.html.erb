<p id="notice"><%= notice %></p>

<h1>Your Portfolio</h1>
<br/>
<table>
  <thead>
    <tr>
      <th>Ticker</th>
      <th>Shares</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @portfolio.each do |ticker| %>
      <tr>
        <td><%= ticker[0] %></td>
        <td><%= ticker[1] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

